<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root fx:id="mainDialog" maxHeight="600.0" maxWidth="600.0" prefHeight="187.0" prefWidth="406.0" style="-fx-background-color: #1A237E; -fx-border-color: #666666;" type="VBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
        <children>
                <HBox prefHeight="106.0" prefWidth="406.0" style="-fx-padding: 30; -fx-border-color: #d3d3d3; -fx-border-width: 0 0 .75 0;">
                        <children>
                                <Text fx:id="lblContentMsg" fill="WHITE" fontSmoothingType="LCD" strokeType="OUTSIDE" strokeWidth="0.0" text="Save Successfully">
                                        <font>
                                                <Font name="System Bold" size="17.0" />
                                        </font>
                                </Text>
                        </children>
                </HBox>
                <HBox alignment="BOTTOM_RIGHT" prefHeight="84.0" prefWidth="406.0" spacing="40.0" style="-fx-padding: 30;" VBox.vgrow="ALWAYS">
                        <children>
                                <Button fx:id="btnOk" mnemonicParsing="false" prefHeight="33.0" prefWidth="108.0" style="-fx-background-color: transparent; -fx-font-size: 15; -fx-font-weight: bold; -fx-border-color: #FFEB3B; -fx-border-radius: 5; -fx-background-radius: 5;" text="Confirm" textFill="#69ffea" visible="false" />
                                <Button fx:id="btnClose" mnemonicParsing="false" prefHeight="33.0" prefWidth="77.0" style="-fx-background-color: transparent; -fx-font-size: 15; -fx-font-weight: bold; -fx-border-color: #FFEB3B; -fx-border-radius: 5; -fx-background-radius: 5;" text="Ok" textFill="WHITE" HBox.hgrow="ALWAYS" />
                        </children>
                </HBox>
        </children>
        <effect>
                <DropShadow color="#323232" />
        </effect>
</fx:root>
